<div class="department-form">
  <div class="title">新規スケジュール</div>
  <%=form_with model: @department, local: true do |f|%>
    <div class="department-format">
      <div class="form-group side">
        <%= f.label :部署・所属,for: "formGroupExampleInput", class: 'department-form-label'%>
        <%= f.text_field :name, class: 'department-form-input form-control', id:"formGroupExampleInput", placeholder: '部署・所属名を入力してください' %>
      </div>

      <div class="form-group">
          <label class='department-form-label' for='chat_room_チャットメンバー'>メンバー</label>
      </div>

      <%= f.submit class: 'department-form_action-btn btn btn-primary', value: "登録"%>
    </div>
  <% end %>
</div>